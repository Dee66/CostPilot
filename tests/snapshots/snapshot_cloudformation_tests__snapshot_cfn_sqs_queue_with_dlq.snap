---
source: tests/snapshot_cloudformation_tests.rs
expression: template
---
{
  "AWSTemplateFormatVersion": "2010-09-09",
  "Resources": {
    "DeadLetterQueue": {
      "Properties": {
        "MessageRetentionPeriod": 1209600,
        "QueueName": "events-dlq"
      },
      "Type": "AWS::SQS::Queue"
    },
    "MainQueue": {
      "Properties": {
        "QueueName": "events",
        "RedrivePolicy": {
          "deadLetterTargetArn": {
            "Fn::GetAtt": [
              "DeadLetterQueue",
              "Arn"
            ]
          },
          "maxReceiveCount": 3
        },
        "VisibilityTimeout": 300
      },
      "Type": "AWS::SQS::Queue"
    }
  }
}
